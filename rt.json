{
  "release_notes": {
    "commit_format": "* :hash: :message",
    "title": "Release :version"
  },
  "before_release": [
    "go build -ldflags='-w -s -X github.com/redwebcreation/nest/globals.Version=:version' -gcflags=all='-l'"
  ],
  "after_release": [
    "rm nest"
  ],
  "assets": [
    "./nest"
  ],
  "tag_message": "Version :tag"
}